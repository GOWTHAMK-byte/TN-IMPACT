# ServiceHub - Environment Configuration
# Copy this file to .env and update with your values

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/servicehub

# JWT Secrets (change these in production!)
JWT_SECRET=servicehub-dev-secret-change-in-prod
JWT_REFRESH_SECRET=servicehub-refresh-secret-change-in-prod

# Server
PORT=5000
NODE_ENV=development
